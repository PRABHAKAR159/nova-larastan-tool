{
    "name": "ijpatricio/nova-larastan-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "nunomaduro/larastan": "*"
    },
    "autoload": {
        "psr-4": {
            "Ijpatricio\\NovaLarastanTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ijpatricio\\NovaLarastanTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
